<view class="RegisterView" style="{{'height:'+(height)+';'}}"><image class="background" style="{{'height:'+(height)+';'}}" mode="aspectFill" src="../../static/LoginAndRegister/background2.jpg"></image><view class="title" style="{{'padding-top:'+(sysheight/9+'px')+';'}}"><block wx:if="{{isLogin}}"><view class="title-Ico"><text>萌二</text></view></block><text class="title-text"><text>登录萌二</text></text></view><view class="input-box show" style="{{'margin-top:'+(sysheight/5.5+'px')+';'+('height:'+(sysheight/2.74+'px')+';')}}"><view class="input-box-content" style="{{'height:'+(sysheight/3.146+'px')+';'}}"><view class="password itemView"><image class="password-ico" src="../../static/LoginAndRegister/myself.png"></image><input class="inputs" placeholder="请输入密码" placeholder-class="placeholderClass"/></view><view class="check-password itemView"><image class="password-ico" src="../../static/LoginAndRegister/lock.png"></image><input class="inputs" placeholder="请确认密码" placeholder-class="placeholderClass"/></view><view class="forget">忘记密码</view><view class="Register-submit"><view data-event-opts="{{[['tap',[['goIndex',['$event']]]]]}}" class="Register-submit-btn" bindtap="__e"><text>登录</text></view></view></view><view class="GoToRegister"><view><text>没有账号？</text><text data-event-opts="{{[['tap',[['gotoRegister',['$event']]]]]}}" class="goto" bindtap="__e">点击注册</text></view></view></view><view class="problem"><text>遇到问题 | 寻求帮助</text></view></view>