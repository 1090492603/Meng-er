<view><view class="status" style="{{'position:'+(headerPosition)+';'}}"></view><view class="header" style="{{'position:'+(headerPosition)+';'}}"><view class="scan"><view data-event-opts="{{[['tap',[['scan',['$event']]]]]}}" class="icon scan" style="color:#808080;margin-top:15rpx;" bindtap="__e"></view></view><view class="input"><view class="icon search"></view><input data-event-opts="{{[['tap',[['toSearch']]]]}}" style="background-color:#000000;opacity:0.05;color:#000000;" bindtap="__e"/></view><view class="menu"><image style="width:50rpx;height:50rpx;" mode="widthFix" src="../../static/HM-shophome/face.png" data-event-opts="{{[['tap',[['shopUser',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="place"></view><view class="swiper-view"><swiper class="swiper" indicator-dots="true" autoplay="true" circular="true" indicator-active-color="#ffffff"><block wx:for="{{swiperList}}" wx:for-item="swiper" wx:for-index="__i0__" wx:key="sid"><swiper-item data-event-opts="{{[['tap',[['toSwiper',['$0'],[[['swiperList','sid',swiper.sid]]]]]]]}}" bindtap="__e"><image mode="aspectFill" src="{{swiper.img}}"></image></swiper-item></block></swiper></view><view class="category"><view class="box"><swiper class="swiper" style="height:72px;" duration="300"><block wx:for="{{categoryList}}" wx:for-item="page" wx:for-index="pageindex" wx:key="pageindex"><swiper-item><view class="category-list"><block wx:for="{{page}}" wx:for-item="category" wx:for-index="__i1__" wx:key="cat_id"><view data-event-opts="{{[['tap',[['toCategory',['$0'],[[['categoryList','',pageindex],['','cat_id',category.cat_id]]]]]]]}}" class="icon" bindtap="__e"><image mode="widthFix" src="{{category.img}}"></image><view>{{category.title}}</view></view></block></view></swiper-item></block></swiper></view></view><view class="banner"><image mode="widthFix" src="../../static/HM-shophome/banner.jpg"></image></view><view class="goods-list"><view class="title">— 猜你喜欢 —</view><view class="product-list"><block wx:for="