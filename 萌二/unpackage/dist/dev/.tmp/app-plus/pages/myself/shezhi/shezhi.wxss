
.body{
	font-family: "\9ED1\4F53";
	padding: 0;
	margin: 0;
}
.head{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	position: absolute;
	top: 0;
	width: 96.7%;
	padding: 40rpx 0 20rpx 20rpx;
	background: #FFFFFF;
}
.headBack{
	width: 39rpx;
	height: 39rpx;
}
.headText{
	padding-left: 38%;
	font-size: 38rpx;
	font-weight: 500;
}
.zhanWei{
	background:  #EFF4FF;
	height: 120rpx;
}
.midBox{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	padding: 30rpx 0 30rpx 15rpx;
}
.forward{
	position: fixed;
	right: 30rpx;
	width: 40rpx;
	height: 40rpx;
}
.midText{
	font-size: 28rpx;
	font-weight: 400;
	color: #333333;
}
.btnBox{
	width: 250rpx;
	height: 80rpx;
	background: #32B9d6;
	margin: 40rpx 0 0 37%;
	border-radius: 40rpx;
}
.btntext{
	position: relative;
	top: 20%;
	left: 20%;
	color: #FFFFFF;	
	width: 100%;
}

